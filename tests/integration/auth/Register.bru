meta {
  name: Register
  type: http
  seq: 2
}

post {
  url: {{host}}/api/auth/register
  body: formUrlEncoded
  auth: inherit
}

body:form-urlencoded {
  email: asd@asd.com
  phone: 8800
  password: 123
}

body:multipart-form {
  email: asd@asd.com
  password: 123
}

tests {
  test("Return 200", () => {
    expect(res.status).to.equal(200);
  });
  
  test("Content type is JSON", () => {
    expect(res.getHeader("content-type")).to.contain("application/json");
  });
  
  test("Returned token", () => {
    expect(res.token).to.be.a.string;
  });
}

settings {
  encodeUrl: true
  timeout: 0
}
